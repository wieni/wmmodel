{
    "name": "wieni/wmmodel",
    "type": "drupal-module",
    "description": "Adds support for bundle-specific models for Drupal 8 entities.",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "drupal/core": "^9.3"
    },
    "require-dev": {
        "drupal/core-dev": "^9.3",
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.3"
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9 || ^10"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php",
            "phpstan analyse"
        ]
    }
}
